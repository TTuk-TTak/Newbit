<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.newbit.model.mapper.InfoMapper">

	<select id="getMembers" resultType="int">
		select count(*)
		from user
	</select>

	<select id="getContents" resultType="int">
		select count(*)
		from content
	</select>

	<select id="getPosts" resultType="int">
		select count(*)
		from post
	</select>

</mapper>